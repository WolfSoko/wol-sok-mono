{
  "name": "rollapolla",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "wolsok",
  "sourceRoot": "apps/rollapolla/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/rollapolla",
        "index": "apps/rollapolla/src/index.html",
        "main": "apps/rollapolla/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/rollapolla/tsconfig.app.json",
        "assets": ["apps/rollapolla/src/favicon.ico", "apps/rollapolla/src/assets"],
        "styles": ["apps/rollapolla/src/styles.scss"],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "fileReplacements": [
            {
              "replace": "apps/rollapolla/src/environments/environment.ts",
              "with": "apps/rollapolla/src/environments/environment.development.ts"
            }
          ]
        },
        "prototype": {
          "fileReplacements": [
            {
              "replace": "apps/rollapolla/src/environments/environment.ts",
              "with": "apps/rollapolla/src/environments/environment.prototype.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "rollapolla:build:production"
        },
        "development": {
          "browserTarget": "rollapolla:build:development"
        },
        "prototype": {
          "browserTarget": "rollapolla:build:prototype"
        }
      },
      "defaultConfiguration": "development"
    },
    "firebase": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/rollapolla",
        "command": "firebase"
      },
      "configurations": {
        "login": {
          "command": "firebase login"
        },
        "deploy": {
          "command": "firebase deploy"
        },
        "emulators": {
          "command": "firebase emulators:exec"
        }
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/rollapolla",
        "command": "firebase deploy"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "rollapolla:build"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/rollapolla/**/*.ts", "apps/rollapolla/**/*.html"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/rollapolla/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "rollapolla:build"
      },
      "configurations": {
        "prototype": {
          "buildTarget": "rollapolla:build:prototype"
        }
      }
    }
  }
}
