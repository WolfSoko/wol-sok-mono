{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/angular-examples/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/angular-examples",
        "index": "apps/angular-examples/src/index.html",
        "main": "apps/angular-examples/src/main.ts",
        "polyfills": "apps/angular-examples/src/polyfills.ts",
        "tsConfig": "apps/angular-examples/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "customWebpackConfig": {
          "path": "tools/version-handling/version-webpack.config.js"
        },
        "assets": [
          {
            "glob": "*.*",
            "input": "dist/libs/fib-wasm/",
            "output": "fib-wasm"
          },
          "apps/angular-examples/src/favicon.ico",
          "apps/angular-examples/src/manifest.json",
          "apps/angular-examples/src/assets"
        ],
        "styles": ["apps/angular-examples/src/styles.scss"],
        "stylePreprocessorOptions": {
          "includePaths": [
            "apps/angular-examples/src/styles",
            "libs/ui-kit/src/lib/styles"
          ]
        },
        "scripts": [
          "node_modules/vega-lite/build/vega-lite.js",
          {
            "input": "node_modules/document-register-element/build/document-register-element.js",
            "inject": true
          }
        ],
        "allowedCommonJsDependencies": [
          "assemblyscript/lib/loader",
          "three-orbitcontrols-ts",
          "gpu.js",
          "semver",
          "firebase/app",
          "@firebase/component",
          "@firebase/auth",
          "typed-function",
          "decimal.js",
          "fraction.js",
          "complex.js",
          "lodash",
          "p5",
          "html2canvas",
          "seedrandom"
        ],
        "buildLibsFromSource": false,
        "sourceMap": true
      },
      "configurations": {
        "production": {
          "sourceMap": {
            "hidden": true,
            "scripts": true,
            "styles": true
          },
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2.48mb",
              "maximumError": "2.5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/angular-examples/src/environments/environment.ts",
              "with": "apps/angular-examples/src/environments/environment.prod.ts"
            }
          ],
          "serviceWorker": true,
          "ngswConfigPath": "apps/angular-examples/ngsw-config.json",
          "outputHashing": "all"
        },
        "development": {
          "preserveSymlinks": true,
          "buildOptimizer": true,
          "optimization": true,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/web:file-server",
      "options": {
        "buildTarget": "angular-examples:build",
        "parallel": true,
        "spa": true
      },
      "configurations": {
        "production": {
          "buildTarget": "angular-examples:build:production"
        },
        "development": {
          "buildTarget": "angular-examples:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "angular-examples:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/angular-examples/src/**/*.ts",
          "apps/angular-examples/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/angular-examples"],
      "options": {
        "jestConfig": "apps/angular-examples/jest.config.ts",
        "passWithNoTests": false
      }
    }
  },
  "tags": ["app"]
}
