      "@wolsok/feat-reaction-diff": [
        "libs/features/lazy/reaction-diffusion/src/index.ts"
      ],
      "@wolsok/feat-shader-examples": [
        "libs/features/lazy/shader-examples/src/index.ts"
      ],
      "@wolsok/feat-shared-reaction-diffusion-kernels": [
        "libs/features/reaction-diffusion-kernels/src/index.ts"
      ],
      "@wolsok/feat-wasm-test": ["libs/features/lazy/wasm-test/src/index.ts"],
      "@wolsok/fib-wasm-api": ["libs/fib-wasm-api/src/index.ts"],
      "@wolsok/ui-kit": ["libs/ui-kit/src/index.ts"],
      "@wolsok/utils-decorators": ["libs/utils/decorators/src/index.ts"],
      "@wolsok/utils-gpu-calc": ["libs/utils/gpu-calc/src/index.ts"],
      "@wolsok/utils-rx-operators": ["libs/utils/rx-operators/src/index.ts"],
      "@wolsok/ws-thanos": ["libs/public/ws-thanos/src/index.ts"]
