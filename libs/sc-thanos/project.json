{
  "projectType": "library",
  "root": "libs/sc-thanos",
  "sourceRoot": "libs/sc-thanos/src",
  "prefix": "ws-thanos",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/sc-thanos"],
      "options": {
        "project": "libs/sc-thanos/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/sc-thanos/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/sc-thanos/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/sc-thanos"],
      "options": {
        "jestConfig": "libs/sc-thanos/jest.config.js",
        "passWithNoTests": false
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/sc-thanos/src/**/*.ts",
          "libs/sc-thanos/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["shared", "published", "ui"]
}
