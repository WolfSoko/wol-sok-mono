{
  "projectType": "library",
  "root": "libs/ui-kit/elem-resized",
  "sourceRoot": "libs/ui-kit/elem-resized/src",
  "prefix": "sh",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/ui-kit/elem-resized"],
      "options": {
        "project": "libs/ui-kit/elem-resized/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ui-kit/elem-resized/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ui-kit/elem-resized/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ui-kit/elem-resized"],
      "options": {
        "jestConfig": "libs/ui-kit/elem-resized/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ui-kit/elem-resized/src/**/*.ts",
          "libs/ui-kit/elem-resized/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["ui", "shared"]
}
