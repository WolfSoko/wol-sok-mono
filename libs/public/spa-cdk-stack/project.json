{
  "name": "spa-cdk-stack",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/public/spa-cdk-stack/src",
  "projectType": "library",
  "tags": ["infra-shared", "published"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/public/spa-cdk-stack",
        "main": "libs/public/spa-cdk-stack/src/index.ts",
        "tsConfig": "libs/public/spa-cdk-stack/tsconfig.lib.json",
        "assets": ["libs/public/spa-cdk-stack/*.md"]
      }
    },
    "test": {
      "executor": "@nx/vite:test"
    },
    "deploy": {
      "executor": "nx:run-commands",
      "inputs": [
        "production",
        "^production",
        "{projectRoot}/package.json"
      ],
      "dependsOn": ["build"],
      "options": {
        "command": "npm publish dist/libs/public/spa-cdk-stack --access public"
      },
      "configurations": {
        "dry-run": {
          "command": "npm publish dist/libs/public/spa-cdk-stack --access public --dry-run"
        }
      }
    }
  }
}
