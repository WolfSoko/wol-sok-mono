{
  "name": "ws-thanos",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/public/ws-thanos/src",
  "prefix": "ws",
  "targets": {
    "build": {
      "executor": "@nx/angular:ng-packagr-lite",
      "outputs": ["{workspaceRoot}/dist/libs/public/ws-thanos"],
      "options": {
        "project": "libs/public/ws-thanos/ng-package.json",
        "updateBuildableProjectDepsInPackageJson": true
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/public/ws-thanos/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/public/ws-thanos/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["libs/public/ws-thanos/src/**/*.ts", "libs/public/ws-thanos/src/**/*.html"]
      }
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "libs/public/ws-thanos/src/test.ts",
        "tsConfig": "libs/public/ws-thanos/tsconfig.spec.json",
        "karmaConfig": "libs/public/ws-thanos/karma.conf.js"
      }
    },
    "release": {
      "executor": "nx:run-commands",
      "outputs": [],
      "options": {
        "command": "npx semantic-release-plus --extends ./libs/public/ws-thanos/release.config.js",
        "parallel": false
      }
    }
  },
  "tags": ["shared", "published", "ui"]
}
