{
  "nxCloudUrl": "https://cloud.nx.app",
  "nxCloudAccessToken": "ZDIxOTJiNDctZjhhNS00NDYzLWFiNzQtODg0NDJkYTMyYzBlfHJlYWQtb25seQ==",
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "pluginsConfig": {
    "@nx/js": {
      "analyzeSourceFiles": true
    }
  },
  "plugins": [
    {
      "plugin": "@nx/playwright/plugin",
      "options": {
        "targetName": "e2e"
      }
    },
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    }
  ],
  "namedInputs": {
    "default": ["{projectRoot}/**/*"],
    "production": [
      "default",
      "!{projectRoot}/**/*.spec.[jt]s",
      "!{projectRoot}/tsconfig.*spec.json",
      "!{projectRoot}/karma.conf.js",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/.eslintrc.json",
      "!{projectRoot}/.storybook/**/*",
      "!{projectRoot}/**/*.stories.@(js|jsx|ts|tsx|mdx)",
      "!{projectRoot}/tsconfig.storybook.json",
      "!{projectRoot}/src/*test-setup.[jt]s",
      "!{projectRoot}/cypress/**/*",
      "!{projectRoot}/**/*.cy.[jt]s?(x)",
      "!{projectRoot}/cypress.config.[jt]s",
      "!{projectRoot}/tsconfig.spec.json"
    ],
    "globals": [],
    "version": [
      {
        "runtime": "node tools/version-handling/version-webpack.config.js"
      }
    ]
  },
  "targetDefaults": {
    "release": {
      "dependsOn": ["build"]
    },
    "deploy": {
      "dependsOn": ["^build"]
    },
    "build": {
      "inputs": ["production", "globals", "^production", "^globals"],
      "dependsOn": ["^build"],
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "cache": true
    },
    "e2e": {
      "inputs": ["default", "^production"],
      "outputs": [
        "{workspaceRoot}/test-reports/{projectRoot}",
        "{workspaceRoot}/dist/.playwright/{projectRoot}"
      ],
      "cache": true
    },
    "test": {
      "inputs": [
        "default",
        "^production",
        "{workspaceRoot}/karma.conf.js",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/jest.config.ts",
        "{workspaceRoot}/.codecov.yml"
      ],
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "cache": true,
      "configurations": {
        "ci": {
          "codeCoverage": true
        }
      }
    },
    "test-integration": {
      "inputs": ["default", "^production", "{workspaceRoot}/karma.conf.js"],
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "cache": true,
      "configurations": {
        "ci": {
          "codeCoverage": true
        }
      }
    },
    "prepare-prod": {
      "dependsOn": ["build"],
      "cache": true
    },
    "build-storybook": {
      "inputs": [
        "default",
        "^production",
        "{projectRoot}/.storybook/**/*",
        "{projectRoot}/tsconfig.storybook.json"
      ],
      "cache": true
    },
    "component-test": {
      "inputs": ["default", "^production"],
      "cache": true
    },
    "@nx/jest:jest": {
      "inputs": [
        "default",
        "^production",
        "{workspaceRoot}/karma.conf.js",
        "{workspaceRoot}/jest.preset.js",
        "{workspaceRoot}/jest.config.ts",
        "{workspaceRoot}/.codecov.yml"
      ],
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "cache": true,
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "@nx/eslint:lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"],
      "cache": true,
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["{projectRoot}/**/*.ts", "{projectRoot}/**/*.html"]
      }
    },
    "@nx/angular:webpack-browser": {
      "inputs": [
        {
          "env": "NX_MF_DEV_SERVER_STATIC_REMOTES"
        }
      ]
    },
    "@nx/js:tsc": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"]
    },
    "@nx/vite:test": {
      "cache": true,
      "inputs": ["default", "^production"]
    }
  },
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "packageManager": "npm"
  },
  "generators": {
    "@nx/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nx/angular:component": {
      "style": "scss"
    }
  },
  "release": {
    "projects": ["tag:published"],
    "projectsRelationship": "independent",
    "changelog": {
      "projectChangelogs": true,
      "workspaceChangelog": {
        "createRelease": "github"
      }
    },

    "version": {
      "conventionalCommits": true
    }
  },
  "defaultProject": "angular-examples",
  "parallel": 3
}
